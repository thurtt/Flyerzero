<div id="board_page">
	<%= render :partial=>"flyer"%>
</div>

<div id="submission_page">
	<div style="width:990px; margin:50px auto;height: 100%;padding-bottom:40px;" id="form_area">
		<div id="form_content">
			<%=form_for( @event, :url=>{ :controller=>"events",:action=>"create" }, :remote=>"true" ) do |f| %>
				<div id="dragdrop" class="container"><%= render :partial=>"events/dragdrop" %></div>
				<div id="submit" class="container"><%= render :partial=>"events/submit", :locals=>{ :f=>f } %></div>
			<%end%>
		</div>
		<%= render :partial=>"events/message_panel" %>
	</div>

</div>

<div id="about_page">

	<%= render :partial=>"about" %>

</div>