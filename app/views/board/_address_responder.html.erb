<script>
	user_latitude = <%=@origin.ll.split(',')[0]%>;
	user_longitude = <%=@origin.ll.split(',')[1]%>;
	findCountry(user_latitude, user_longitude);
	latitude = <%=@origin.ll.split(',')[0]%>;
	longitude = <%=@origin.ll.split(',')[1]%>;
	loadFlyerData('<%=@origin.ll.split(',')[0]%>', '<%=@origin.ll.split(',')[1]%>');
	
	$('#address').attr('href','https://maps.googleapis.com/maps/api/staticmap?center='+latitude+','+longitude+'&zoom=14&size=400x400&sensor=false');
	$('.map_image').attr('src','https://maps.googleapis.com/maps/api/staticmap?center='+latitude+','+longitude+'&zoom=14&size=400x400&sensor=false');
	
</script>
